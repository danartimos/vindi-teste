{% extends 'base.html.twig' %}

{% block stylesheets %}
    <style>
        body {
    margin: 40px 10px;
    padding: 0;
    font-family: Arial, Helvetica Neue, Helvetica, sans-serif;
    font-size: 14px;
  }
        #calendar {
            max-width: 900px;
            margin: 0 auto;
        }
    </style>
{% endblock %}

{% block body %}
    <div id="calendar"></div>
{% endblock %}

{% block javascripts %}
    <script>
        var logado = '{{logado['in']|default('false')}}';
        var eventos = decodeURI('{{eventos}}');
    </script>
    <script src="{{ asset('js/calendario.js') }}"></script>
{% endblock %}